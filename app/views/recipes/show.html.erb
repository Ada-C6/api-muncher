<p><%= @recipe.label %></p>
<p><%= image_tag(@recipe.photo, alt: @recipe.label) %></p>
<p><%= link_to "Original recipe", @recipe.link, target: "_blank" %></p>
<p><%= "Ingredients:" %></p>
<ul><% @recipe.ingredients.each do |i| %>
  <li><%= i["text"] %></li>
  <% end %>
</ul>
<p><%= "Dietary Information:" %></p>
<ul><% @recipe.dietary_info.each do |d| %>
  <li><%= d %></li>
  <% end %>
</ul>
