<%=image_tag @recipe.photo %>

<table>
  <thead>
    <tr>
      <th>
        <h1><%=@recipe.name %></h1>
      </th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>
        <h3>Ingredients</h3>
      </td>
    </tr>
    <%@recipe.ingredientLines.each do |line| %>
    <tr>
      <td>
          <%=line%>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>

<p>
  <a href="<%= @recipe.link%>" target="_blank"><%= "Link to Edamam Recipe" %></a>
</p>

<p>
  <a href="<%=@recipe.url%> "> <%= "Link to Original Recipe" %></a>
</p>
