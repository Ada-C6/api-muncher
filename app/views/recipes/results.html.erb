<article>
  <section>
    <table>
      <% @recipes.each do |recipe| %>
      <tr>
        <td>
          <%= link_to "#{image_tag(recipe.image)}".html_safe, recipe_path(identifier:recipe.identifier) %>
          <%= recipe.label %>
          From: <%= recipe.source %>
        </td>
      </tr>
      <% end %>
    </table>
  </section>
</article>
