<h1>YOUR RESULTS</h1>


<% @searched.each do |recipe| %>
  <%= link_to "#{image_tag(recipe.photo)}".html_safe, show_recipe_path(recipe.label), class: "category-img" %>
  <p><%= recipe.label %></p>
  <p><%= recipe.link %></p>
  <p><%= link_to recipe.label, show_recipe_path(recipe.label) %></p>
<% end %>
