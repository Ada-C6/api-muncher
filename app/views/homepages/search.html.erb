<h1>Search Result</h1>
<% @recipes.each do |recipe| %>
  <h4><%= recipe.label %></h4>
  <%= image_tag "#{recipe.image}", :class=>'recipe_photo' %>
  <p><%= link_to "Details", recipe.url %></p>
<% end %>

<%= render :partial => 'pagination', :locals => {
  :term => @term,
  :count => @count,
  :more => @more } %>
